struct colors {
  uint8_t CRed;
  uint8_t CGreen;
  uint8_t CBlue;
};
uint8_t newR;
uint8_t newG;
uint8_t newB;
struct colors hue2rgb(int hue){
  newR = 0;
  newB = 0;
  newG = 0;
  struct colors outcolors;
  switch (hue/60){
      case 0: //cycle 1- R max G inc B 0 where hue < 60
      newR = 255;
      newG = (hue % 60) * 4.25; //4.25 is the ratio of hue:rgb 
      break;
      case 1: //cycle 2 - R dec G max B 0, 60 <= hue < 120 
      newR = 255 - (hue % 60) * 4.25;
      newG = 255; 
      break;
      case 2: //cycle 3 - R 0 G max B inc, 120 <= hue < 180
      newG = 255;
      newB = (hue % 60) * 4.25;
      break;
      case 3: //cycle 4 - r 0 G dec B max, 180 <= hue < 240
      newG = 255 - (hue % 60) * 4.25;
      newB = 255;
      break;
      case 4: //cycle 5 - R inc G 0 B max, 240 <= hue < 300
      newR = (hue % 60) * 4.25;
      newB = 255;
      break;
      case 5: //cycle 6 - B decreases to oblivion and you know, hue >= 300
      newR = 255;
      newB = 255 - (hue % 60) * 4.25;
      break;
     }
  outcolors.CRed = newR;
  outcolors.CGreen = newG;
  outcolors.CBlue = newB;
  return outcolors;
}


int colorMap[64]; //this is an array containing hue values, which is mapped every color shift based on hueOffset, 64 elements
int loadPixel[3];
struct colors readcolors;
void draw_face(const uint8_t ani[], int hueOffset){
  int imageHeight = 32;
  int imageWidth = 64;
 
  //x = 0 to 64 (1 face, turns out to be the right)
  for (int i = 0; i < imageWidth; i++)
  {
    hue = ((i + hueOffset) * (360/imageWidth)) % 360;
    colorMap[i] = hue;
  } 
  for (int r = 0; r < 2048; r++){ //draw pixel 
    if (ani[r] != 0){
      readcolors = hue2rgb(colorMap[r % 64]);
      display.drawPixelRGB888(r % imageWidth, r / imageWidth, readcolors.CRed, readcolors.CGreen, readcolors.CBlue);
    }
  }
  //this loop repeats again for the second face, x = 63 + 64 in the first pixel to the left and decreases to the right
  for (int r = 0; r < 2048; r++){ //mirrored face
    if (ani[r] != 0){
      readcolors = hue2rgb(colorMap[r % 64]);
      display.drawPixelRGB888(r % imageWidth + 2 * (64 - r % imageWidth) - 1, r / imageWidth, readcolors.CRed, readcolors.CGreen, readcolors.CBlue);
    }
  }
}

